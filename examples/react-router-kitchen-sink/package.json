{
  "name": "react-router-kitchen-sink",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "yarn run build.client && yarn run build.server",
    "build.client": "NODE_ENV=production vite build",
    "build.server": "NODE_ENV=production vite build --ssr",
    "start": "NODE_ENV=production node dist/server.js --port 3000",
    "start.bun": "NODE_ENV=production bun run dist/server.js",
    "typecheck": "tsc",
    "--db related commands below--": "",
    "db.push": "drizzle-kit push:sqlite",
    "migrate.up": "tsx src/server/db/migrate.ts",
    "migrate.generate": "drizzle-kit generate:sqlite",
    "migrate.check": "drizzle-kit check:sqlite",
    "debug": "tsx debug.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.12.2",
    "@lucia-auth/adapter-sqlite": "2.0.1",
    "@paralleldrive/cuid2": "2.2.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@ssrx/plugin-react-router": "latest",
    "@ssrx/plugin-tanstack-query": "latest",
    "@ssrx/plugin-trpc-react": "latest",
    "@ssrx/plugin-unhead": "latest",
    "@ssrx/react": "latest",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-query-devtools": "^5.56.2",
    "@trpc/client": "10.45.2",
    "@trpc/server": "10.45.2",
    "better-sqlite3": "11.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "0.33.0",
    "drizzle-valibot": "0.2.0",
    "hono": "4.6.1",
    "lucia": "2.7.7",
    "lucide-react": "^0.441.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.0.13",
    "react-router-dom": "6.26.2",
    "react-router-typesafe-routes": "1.2.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "valibot": "0.41.0"
  },
  "devDependencies": {
    "@ssrx/vite": "latest",
    "@types/better-sqlite3": "7.6.11",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.1",
    "autoprefixer": "10.4.20",
    "drizzle-kit": "0.24.2",
    "postcss": "8.4.45",
    "rollup-plugin-analyzer": "4.0.0",
    "tailwindcss": "3.4.11",
    "tsx": "4.19.1",
    "typescript": "5.6.2",
    "vite": "5.4.5",
    "vite-tsconfig-paths": "5.0.1"
  }
}
