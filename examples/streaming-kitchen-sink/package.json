{
  "name": "streaming-kitchen-sink",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "yarn run build.client && yarn run build.server",
    "build.client": "NODE_ENV=production vite build",
    "build.server": "NODE_ENV=production vite build --ssr",
    "start": "NODE_ENV=production node dist/server.js --port 3000",
    "start.bun": "NODE_ENV=production bun run dist/server.js",
    "typecheck": "tsc",
    "--db related commands below--": "",
    "db.push": "drizzle-kit push:sqlite",
    "migrate.up": "tsx src/server/db/migrate.ts",
    "migrate.generate": "drizzle-kit generate:sqlite",
    "migrate.check": "drizzle-kit check:sqlite",
    "debug": "tsx debug.ts"
  },
  "dependencies": {
    "@decs/typeschema": "0.12.1",
    "@hono/node-server": "1.2.0",
    "@hono/valibot-validator": "0.2.0",
    "@lucia-auth/adapter-sqlite": "2.0.1",
    "@paralleldrive/cuid2": "2.2.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@ssrx/plugin-react-router": "latest",
    "@ssrx/plugin-tanstack-query": "latest",
    "@ssrx/plugin-trpc-react": "latest",
    "@ssrx/plugin-unhead": "latest",
    "@ssrx/react": "latest",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.0.1",
    "@trpc/server": "10.40.0",
    "better-sqlite3": "9.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "drizzle-orm": "0.28.6",
    "drizzle-valibot": "0.1.1",
    "hono": "3.8.1",
    "lucia": "2.7.1",
    "lucide-react": "^0.288.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-router-dom": "6.17.0",
    "react-router-typesafe-routes": "1.2.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "valibot": "0.19.0"
  },
  "devDependencies": {
    "@ssrx/vite": "latest",
    "@types/better-sqlite3": "7.6.6",
    "@types/react": "18.2.29",
    "@types/react-dom": "18.2.14",
    "@vitejs/plugin-react": "4.1.0",
    "autoprefixer": "10.4.16",
    "drizzle-kit": "0.19.13",
    "postcss": "8.4.31",
    "tailwindcss": "3.3.3",
    "tsx": "3.14.0",
    "typescript": "5.2.2",
    "vite": "5.0.0-beta.10",
    "vite-tsconfig-paths": "4.2.1"
  }
}
