{
  "name": "tanstack-router-simple",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "yarn run build.client && yarn run build.server",
    "build.client": "NODE_ENV=production vite build",
    "build.server": "NODE_ENV=production vite build --ssr",
    "start": "node dist/server.js --port 3000",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@hono/node-server": "^1.12.2",
    "@ssrx/plugin-tanstack-router": "latest",
    "@ssrx/react": "latest",
    "@ssrx/vite": "latest",
    "@tanstack/react-router": "1.57.13",
    "@tanstack/router-devtools": "1.57.13",
    "@tanstack/start": "1.57.13",
    "hono": "4.6.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@tanstack/router-plugin": "1.57.13",
    "@types/react": "18.2.51",
    "@types/react-dom": "18.2.18",
    "@vitejs/plugin-react": "4.3.1",
    "autoprefixer": "10.4.17",
    "postcss": "8.4.45",
    "tailwindcss": "3.3.5",
    "typescript": "5.6.2",
    "vite": "5.4.4",
    "vite-tsconfig-paths": "4.3.2"
  }
}
