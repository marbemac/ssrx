{
  "name": "@ssrx/remix",
  "version": "0.2.0",
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "keywords": [
    "remix",
    "ssr"
  ],
  "author": "marbemac (https://github.com/marbemac)",
  "repository": {
    "type": "git",
    "url": "https://github.com/marbemac/ssrx",
    "directory": "packages/remix"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./src/server.tsx",
      "bun": "./src/server.tsx",
      "node": "./dist/server.js",
      "worker": "./dist/server.js",
      "workerd": "./dist/server.js",
      "deno": "./dist/server.js",
      "edge": "./dist/server.js",
      "default": "./dist/client.js"
