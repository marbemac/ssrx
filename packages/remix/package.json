      "bun": "./src/server.tsx",
      "default": "./dist/server.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "typecheck": "tsc",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "@remix-run/react": ">=2.2.0"
  },
  "dependencies": {
    "@ssrx/renderer": "^0.3.0",
    "isbot-fast": "1.2.0"
  },
  "devDependencies": {
    "@remix-run/react": "2.4.1",
    "esbuild": "0.19.10",
    "rollup": "4.9.1",
    "rollup-plugin-esbuild": "6.1.0"
  }
